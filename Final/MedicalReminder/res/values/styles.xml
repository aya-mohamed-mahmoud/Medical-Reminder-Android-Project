<resources xmlns:android="http://schemas.android.com/apk/res/android">    <style name="AppBaseTheme" parent="android:Theme.Light"></style>      <!--  -->    <style name="myTheme" parent="@android:style/Theme.Holo.Light">        <item name="android:actionBarStyle">@style/myTheme.ActionBar</item>          <!--  <item name="android:icon">@drawable/logo</item> -->    </style>    <style name="myTheme.ActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">        <item name="android:background">#1482A7</item>        <item name="android:titleTextStyle">@style/myTheme.ActionBar.Text</item>    </style>    <style name="myTheme.ActionBar.Text" parent="@android:style/TextAppearance">        <item name="android:textColor">#66E066</item>        <item name="android:textSize">20sp</item>    </style>    <!--  --> <style name="SpinnerTextViewItem" parent="@android:style/Widget.TextView" >    <item name="android:textSize">20dp</item>   <!--<item name="android:textStyle">bold</item>-->   <!-- <item name="android:divider">#19A3D1</item>    <item name="android:dividerHeight">3dp</item>    <item name="android:padding">5dp</item>-->    <item name="android:background">@drawable/dropdown_spinner_style</item></style>          <!-- Application theme. -->    <!-- the theme applied to the application or activity -->    <style name="AppTheme" parent="AppBaseTheme">        <!-- All customizations that are NOT specific to a particular API-level can go here. -->        <!-- <item name="android:actionBarStyle">@style/MyActionBar</item> -->    </style>      <!-- text view styling for headers -->    <style name="textViewStyle">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:textColor">#66E066</item>        <item name="android:textSize">18sp</item>        <item name="android:textStyle">bold</item>        <item name="android:padding">7dp</item>    </style>    <!-- buttons -->    <style name="buttonStyle">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:textColor">#66E066</item>        <item name="android:background">#19A3D1</item>        <item name="android:textSize">20sp</item>        <item name="android:textStyle">bold</item>        <item name="android:padding">7dp</item>        <item name="android:layout_margin">10dp</item>    </style>    <!-- Main layout style -->    <style name="mainLayoutStyle">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:layout_gravity">center_horizontal</item>        <item name="android:orientation">vertical</item>        <item name="android:padding">10dp</item>    </style>    <!-- upload image layout style -->    <style name="uploadImageLayoutStyle">        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:layout_gravity">center</item>        <item name="android:orientation">vertical</item>        <item name="android:padding">3dp</item>    </style>    <!-- container layout style -->    <style name="containerLayoutStyle">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:layout_gravity">center_horizontal</item>        <item name="android:layout_margin">10dp</item>        <item name="android:background">#19A3D1</item>        <item name="android:orientation">vertical</item>        <item name="android:padding">7dp</item>    </style>         <!-- alert layout style -->    <style name="alertLayoutStyle">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:layout_gravity">center_horizontal</item>        <item name="android:layout_margin">10dp</item>        <item name="android:orientation">vertical</item>        <item name="android:padding">7dp</item>    </style>    <!-- separator view -->    <style name="separator">        <item name="android:layout_width">fill_parent</item>        <item name="android:layout_height">1dp</item>        <item name="android:background">#66E066</item>    </style>    <!-- inner container that holds one element -->    <style name="clickableHeader">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:clickable">true</item>        <item name="android:orientation">vertical</item>    </style>    <!-- time picker text view style -->    <!--parent="@android:style/Widget.DeviceDefault.DatePicker"-->  <style name="MyTimePicker" >        <item name="android:padding">10dp</item>        <item name="android:textStyle">bold</item>        <item name="android:textSize">16sp</item>        <item name="android:clickable">true</item>        <item name="android:background">#FFFFFF</item>         <item name="android:divider">#19A3D1</item>        <item name="android:textColor">#19A3D1</item>        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>    </style></resources>